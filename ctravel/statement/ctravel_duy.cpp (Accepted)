#include<stdio.h>
#define M 120
#define N 20
using namespace std;
int f[M][M][N];
char s[M][M];
int dx[]={0 ,  0 , 1 ,-1};
int dy[]={1 , -1 , 0 , 0};
int m,n,t;
int R1,C1,R2,C2;
bool inside(int x,int y){
    return ( x >= 0 && x < m && y >= 0 && y < n);
}
int cal(int x,int y,int k){
    if( f[x][y][k] != -1 ) return f[x][y][k];
    if( k == 0 ){
        if( x == R1 && y == C1 ) return f[x][y][k] = 1;
        return f[x][y][k] = 0;
    }
    if(s[x][y] == '*') return f[x][y][k] = 0;
    int res = 0;
    for(int i = 0 ; i < 4 ; i++){
        int cx = x + dx[i];
        int cy = y + dy[i];
        if(inside(cx,cy)) res+=cal(cx,cy,k-1);
    }
    return f[x][y][k] = res;
}
void read(){
    scanf("%d %d %d",&m,&n,&t);
    for(int i = 0 ; i < m ; i++)    scanf("%s",&s[i]);
    for(int i = 0 ; i <= m ; i++)
        for(int j = 0 ; j <= n ; j++)
            for(int k = 0 ; k <= t ; k++)
                f[i][j][k] = -1;
    scanf("%d %d %d %d",&R1,&C1,&R2,&C2);
}
void solve(){
    R1-- ; C1-- ; R2-- ; C2--;
    printf("%d",cal(R2,C2,t));
}
main(){
    //freopen("test.inp","r",stdin);
    read();
    solve();
}
