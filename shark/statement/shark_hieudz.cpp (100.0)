#include<bits/stdc++.h>
using namespace std;
int pos[1234],x[1234],y[1234],z[1234],save[1234];
bool forbid[1234],visited[1234];
vector <int> a[1234];
bool match(int x)
{
    int i;
    if(visited[x]) return false;
    visited[x] = true;
    for(;save[x]<a[x].size();save[x]++)
    {
        if(pos[a[x][save[x]]]==0)
        {
            pos[a[x][save[x]]]=x;
            return true;
        }

    }
    for(i=0;i<a[x].size();i++)
    {
        if(!forbid[a[x][i]])
        {
            forbid[a[x][i]]=true;
            if(match(pos[a[x][i]]))
                {
                    pos[a[x][i]]=x;
                    forbid[a[x][i]]=false;
                    return true;
                }
            forbid[a[x][i]]=false;
        }
    }
    return false;
}
int main()
{
    int n,i;
    //freopen("test.inp","r",stdin);
    scanf("%d",&n);
    for(i=1;i<=n;i++)
        scanf("%d",&x[i]);
    for(i=1;i<=n;i++)
        scanf("%d",&y[i]);
    for(i=1;i<=n;i++)
        scanf("%d",&z[i]);
    for(i=1;i<n;i++)
    for(int j=i+1;j<=n;j++)
    {
        if(x[j]>=x[i] && y[j]>=y[i] && z[j]>=z[i])
        {
            a[j*2].push_back(i);
            a[j*2-1].push_back(i);
            continue;
        }
        swap(i,j);
        if(x[j]>=x[i] && y[j]>=y[i] && z[j]>=z[i])
        {
            a[j*2].push_back(i);
            a[j*2-1].push_back(i);
        }
        swap(i,j);
    }
    int cnt=0;
    for(i=1;i<=n*2;i++)
    {
        memset(visited,false,sizeof visited);
        match(i);
    }
    for(i=1;i<=n;i++)
    {
        if(!pos[i])
            cnt++;
    }
    printf("%d",cnt);
}
