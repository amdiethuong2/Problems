#include<cstdio>
#include<algorithm>
#define f(i,n) for(i=1;i<=n;i++)
#define ff(i,a,b) for(i=a;i<=b;i++)
using namespace std;
int i,n,d[30005],ans,cnt;

int main(void)
{
    scanf("%d",&n);
    f(i,n)
    {
        scanf("%d",&d[i]);
        if(d[i]==1) cnt++;
    }
    ans=cnt;

    f(i,n)
    {
        if(d[i]==2) cnt++;else cnt--;
        if(cnt<ans) ans=cnt;
    }
    printf("%d",ans);
}
