#include <bits/stdc++.h>
using namespace std;
bool check[200],done[200],have[200][200];
vector<int> res,adj[200];
int to[200];
bool multiple;
string word[200];
queue<int> s;
int main()
{
	int n,i,j,pos,u,v;
	ios::sync_with_stdio(false);
	cin>>n;
	for(i=1;i<=n;i++)
	{
		cin>>word[i];
		for(j=0;j<word[i].length();j++)
			check[word[i][j]-'a']=1;
	}
	for(i=1;i<=n;i++)
	{
		for(j=i+1;j<=n;j++)
		{
			for(pos=0;pos<min(word[i].length(),word[j].length());pos++)
			{
				if(word[i][pos]!=word[j][pos])
				{
					if(have[word[i][pos]-'a'][word[j][pos]-'a']) break;
					adj[word[i][pos]-'a'].push_back(word[j][pos]-'a');
					to[word[j][pos]-'a']++;
					have[word[i][pos]-'a'][word[j][pos]-'a']=true;
					//cout<<i<<' '<<j<<' '<<word[i][pos]<<' '<<word[j][pos]<<'\n';
					break;
				}
			}
		}
	}
	for(i='a'-'a';i<='z'-'a';i++)
	{
		if(check[i]&&to[i]==0) s.push(i);
	}
	while(s.size())
	{
		if(s.size()>1) multiple=true;
		u=s.front();
		done[u]=1;
		s.pop();
		res.push_back(u);
		for(i=0;i<adj[u].size();i++)
		{
			v=adj[u][i];
			to[v]--;
			if(to[v]==0)
				s.push(v);
		}
	}
	for(i='a'-'a';i<='z'-'a';i++)
	{
		if(check[i]==true&&done[i]==false)
		{
			cout<<"!";
			return 0;
		}
	}
	if(multiple)
	{
		cout<<"?";
		return 0;
	}
	for(i=0;i<res.size();i++) cout<<char(res[i]+'a');
}