#include<cstdio>
#include<cstring>
#include<algorithm>
using namespace std;
char s[105][15];
bool c[26][26],a[26],cycle=0;
int b[26],r[26],d[26];
void cir(int k)
{
    d[k]=1;
    for(int i=0;i<26;++i)  if(c[k][i])
    {
        if(d[i]==1) cycle=1;
        if(!d[i]) cir(i);
    }
    d[k]=-1;
}
int main()
{
    //freopen("test.inp","r",stdin);
    int n,i,j,dict=0;
    scanf("%d",&n);
    for(i=0;i<n;++i)
    {
        scanf("%s",&s[i]);
        for(j=0;j<strlen(s[i]);++j) a[s[i][j]-'a']=1;
    }
    for(i=1;i<n;++i)
        for(j=0;j<min(strlen(s[i]),strlen(s[i-1]));++j)
            if(s[i-1][j]!=s[i][j])
            {
                c[s[i-1][j]-'a'][s[i][j]-'a']=1;
                break;
            }
    for(i=0;i<26;++i)
        for(j=0;j<26;++j) if(c[i][j]) ++b[j];
    for(i=0;i<26;++i) if(!d[i] && a[i])
    {
        cir(i);
        if(cycle)
        {
            printf("!");
            return 0;
        }
    }
    for(i=0,j=0;i<26;++i) if(a[i]) ++dict;
    for(i=0;i<dict;++i)
    {
        int cnt=0;
        for(j=0;j<26;++j) if(a[j] && !b[j])
        {
            r[i]=j;
            ++cnt;
        }
        if(cnt>1)
        {
            printf("?");
            return 0;
        }
        a[r[i]]=0;
        for(j=0;j<26;++j) if(c[r[i]][j]) --b[j];
    }
    for(i=0;i<dict;++i) printf("%c",r[i]+'a');
}
