{$R-, S-, Q-, I-, O+}

uses math;

var
        n       : longint;
        c       : array[0..25, 0..25] of boolean;
        s       : array[0..99] of string;
        ck, have: array[0..25] of boolean;
        ans     : string;


function toint (ch : char): longint;
begin
        exit (longint(ch) - longint('a'));
end;

function fre (j: longint): boolean;
var
        i       : longint;
begin
        for i := 0 to 25 do if (have[i] = false) and c[j, i] then exit(false);
        exit(true);
end;


var
        i, j, cnt, cc : longint;
        xxx           : boolean;
begin
        assign(input, ''); reset(input);
        assign(output, ''); rewrite(output);

        readln(n);
        for i := 0 to n-1 do
        begin
           readln(s[i]);
           for j := 1 to length(s[i]) do ck[toint(s[i][j])] := true;
           if i <> 0 then
           begin
               for j := 1 to min(length(s[i]), length(s[i-1])) do
               begin
                  if s[i][j] <> s[i-1][j] then
                     begin
                        c[toint(s[i][j]), toint(s[i-1][j])] := true;
                        break;
                     end;
               end;
           end;
        end;
        cc := 0;
        for i := 0 to 25 do if ck[i] then inc(cc);
        for j := 1 to cc do
        begin
          cnt := 0;
          for i := 0 to 25 do if (have[i] = false) and ck[i] and fre(i) then inc(cnt);
          if cnt = 0 then
          begin
             write('!');
             exit;
          end;
          if cnt > 1 then xxx := true;

          for i := 0 to 25 do if (have[i] = false) and ck[i] and fre(i) then
          begin
             have[i] := true;
             ans := ans + char(i+longint('a'));
             break;
          end;

        end;

        if xxx then write('?')
        else write(ans);

        close(input);
        close(output);
end.
