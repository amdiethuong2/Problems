#include <bits/stdc++.h>
using namespace std;

#define II pair<long long,long long>
#define X first
#define Y second
#define PB push_back

vector<long long> f;
int main(){
	//freopen("test.txt","r",stdin);
	long long ans=0,m,n,a,sub;
	cin>>m>>n;
	for(long long i=1;i<=n;i++){
		cin>>a;f.PB(a);
		m-=a;
	}
	if(m>=0){
		cout<<0;return 0;
	}
	m=-m;
	sort(f.begin(),f.end());
	for(long long i=0;i<n;i++){
		sub=min( m/(n-i) , f[i] );
		ans+=sub*sub;
		m-=sub;
	}
	cout<<ans;
}


