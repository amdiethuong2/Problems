#include<cstdio>
#include<algorithm>
using namespace std;
typedef long long ll;
int main()
{
    ll m,n,a[100005],ans=0ll;
    scanf("%lld%lld",&m,&n);
    for(int i=0;i<n;++i)
    {
        scanf("%lld",&a[i]);
        m-=a[i];
    }
    m=-m;
    sort(a,a+n);
    for(ll i=0;i<n;++i)
    {
        ll b=min(a[i],m/(n-i));
        m-=b;
        ans+=1ll*b*b;
    }
    printf("%lld",ans);
}
