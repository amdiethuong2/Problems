#include <cstdio>
#include <vector>
#include <map>
#include <cstdlib>
#define x first
#define y second

using namespace std;

typedef pair<int,int> ii;

int n, res;
ii a[205];
map<ii,bool> sign;

int gcd(int x, int y){
    int k;
    if(x>y){
        k= x; x= y; y= k;
    }
    while(y%x!=0){
        k= y%x;
        y= x;
        x= k;
    }
    return x;
}

ii get(int i, int j){
    int x= a[i].x-a[j].x, y= a[i].y-a[j].y;
    if(x<0){x= -x; y= -y;}
    if(x==0) y= 1;
    else if(y==0) x= 1;
    else{
        int z= gcd(abs(x),abs(y));
        x/= z; y/= z;
    }
    return ii(x,y);
}

int main(){
    scanf("%d", &n);
    for(int i=1;i<=n;i++) scanf("%d%d", &a[i].x, &a[i].y);
    for(int i=1;i<=n;i++){
        for(int j=i+1;j<=n;j++){
            if(a[i]==a[j]) continue;
            ii t= get(i,j);
            if(sign.find(t)==sign.end()){
                res++; sign[t]= true;
            }
        }
    }
    printf("%d\n", res);
    return 0;
}
